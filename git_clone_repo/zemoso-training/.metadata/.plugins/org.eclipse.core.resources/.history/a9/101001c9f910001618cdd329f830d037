import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;


public class triangle {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[][] a ;int i=0,j=0;
		
		try {
			File file = new File("/home/sudhanshu/Desktop/Assignment/git_clone_repo/zemoso-training/triangle.txt");
			Scanner s = new Scanner(file);
			while(s.hasNextLine())
			{
				i++;
				s.nextLine();
				//System.out.println(s.nextLine());
			}
			s = new Scanner(file);
			while(s.hasNextLine()){
				 j++;
				 if(j==100){
					 j=0;
					 while(s.hasNext()){
						 j++;
						 s.next();
					 }
				 }
				 s.nextLine();
			}
			//System.out.println(s.nextLine());
			System.out.println(j);
			System.out.println(i);
			
			a=new int[i][j];
			i=0;j=0;
			
			s = new Scanner(file);

			while(s.hasNextLine()){
				while(s.hasNext()){
					a[i][j]=Integer.parseInt(s.next());
					j++;
					System.out.print(a[i][j]+" ");
				}
				System.out.println();
				i++;
				s.nextLine();
			}

			
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		

	}

}
